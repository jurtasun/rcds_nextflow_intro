{
  "name": "Nextflow Codespace",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  },
  "postCreateCommand": "nextflow -version",
  "remoteUser": "vscode",
  // Configure tool-specific properties.
	"customizations": {
    "vscode": {
      "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter",
    "github.copilot",
    "github.copilot-chat"]
    }
  }
}
